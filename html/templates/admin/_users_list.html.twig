<table class="table">
    <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">user</th>
            <th scope="col">patron</th>
            <th scope="col">roles</th>
        </tr>
    </thead>
    <tbody>
    {% for user in users %}
        <tr>
            <td><a href="/admin/users/{{user.id}}">{{user.id}}</a></td>
            <td>{{user.username}}</td>
            <td>{{user.isSubscribed}}</td>
            <td>
            {% for role in user.roles %}
                {{role}}
            {% endfor %}
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>